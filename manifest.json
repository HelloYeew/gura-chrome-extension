{
  "name": "Gura Chrome Extension",
  "description": "Let's change bad thing to better thing!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "267278.png"
    }
  },
  "icons": {
    "128": "267278.png"
  },
  "options_page": "options.html"
}
